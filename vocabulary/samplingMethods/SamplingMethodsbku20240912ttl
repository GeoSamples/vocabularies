secm:sm006
  rdf:type skos:Concept ;
  dcterm:created "2018-01-30T23:26:29Z"^^xsd:dateTime ;
  dcterm:creator <http://editor.vocabs.ands.org.au/user/GA_Sedgmen.Lara_Admin> ;
  dcterm:source "Geoscience Australia"@en ;
  dcterm:source "SESAR collection method; http://www.deepreef.org/technology/79-rock-dredge.html"@en ;
  rdfs:label "Chain bag dredge"@en ;
  skos:altLabel "dredge chain bag"@en ;
  skos:broader secm:sm033 ;
  skos:definition "A rock dredge is a large bag made of metal chain links attached to a simple heavy frame that keeps the bag expanded. The dredge is lowered off the back of a ship to the seafloor and dragged along to collect large pieces of rock. Because of the coarse chain links  most of the finer material is washed out of the bag  leaving behind the larger rock and gravel pieces within the chain bag. The dredge is then winched up to the surface and any samples are dumped onto the deck of the ship to be sorted and preserved."@en ;
  skos:definition "Sampling of large rocks from the sea floor using a chain bag"@en ;
  skos:exactMatch <gasm:chain_bag_dredge> ;
  skos:inScheme secm:samplingmethods ;
  skos:prefLabel "Chain bag dredge"@en ;
