gasm:chamber_incubation
  rdf:type skos:Concept ;
  dcterm:created "2018-01-30T23:26:29Z"^^xsd:dateTime ;
  dcterm:creator <http://editor.vocabs.ands.org.au/user/GA_Sedgmen.Lara_Admin> ;
  dcterm:source "Geoscience Australia"@en ;
  rdfs:label "Chamber incubation"@en ;
  skos:broader secm:sm200 ;
  skos:definition "A method that isolates sediment and overlying water in a benthic chamber. Water samples are collected from the benthic chamber during the incubation."@en ;
  skos:narrower gasm:chamber_incubation_dark ;
  skos:narrower gasm:chamber_incubation_light ;
  skos:prefLabel "Chamber incubation"@en ;
.
gasm:chamber_incubation_dark
  rdf:type skos:Concept ;
  dcterm:created "2018-01-30T23:26:29Z"^^xsd:dateTime ;
  dcterm:creator <http://editor.vocabs.ands.org.au/user/GA_Sedgmen.Lara_Admin> ;
  dcterm:source "Geoscience Australia"@en ;
  rdfs:label "chamber incubation dark"@en ;
  skos:broader gasm:chamber_incubation ;
  skos:definition "A in situ method that isolates sediment and overlying water in a dark benthic chamber. Incubation water samples are collected from the benthic chamber during the incubation. The dark chamber restricts light penetration and therefore stops the process of photosynthesis. This enables benthic respiration to be measured."@en ;
  skos:prefLabel "chamber incubation dark"@en ;
.
gasm:chamber_incubation_light
  rdf:type skos:Concept ;
  dcterm:created "2018-01-30T23:26:29Z"^^xsd:dateTime ;
  dcterm:creator <http://editor.vocabs.ands.org.au/user/GA_Sedgmen.Lara_Admin> ;
  dcterm:source "Geoscience Australia"@en ;
  rdfs:label "chamber incubation light"@en ;
  skos:broader gasm:chamber_incubation ;
  skos:definition "A in situ method that isolates sediment and overlying water in a light benthic chamber. Incubation water samples are collected from the benthic chamber during the incubation. The  transparent walls of the chamber allow light penetration and enables benthic respiration and photosynthesis to be measured."@en ;
  skos:prefLabel "chamber incubation light"@en ;
.
