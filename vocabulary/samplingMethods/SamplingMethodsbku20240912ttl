secm:waxcoring
  rdf:type skos:Concept ;
  dcterm:source "https://ui.adsabs.harvard.edu/abs/2023AGUFMV43B018.1L/abstract" ;
  dcterm:source "https://web.mit.edu/deeparch/www/publications/papers/YoergerEtAl1999.pdf" ;
  rdfs:label "Wax coring "@en ;
  skos:broader secm:sm026 ;
  skos:definition "Samples sediment and volcanic glass at the sea floor by crashing a weighted core tube that has a ring of stick wax at the bottom into the sea floor.  Invented to sample volcanic glass shards in surface deposits along mid ocean ridges."@en ;
