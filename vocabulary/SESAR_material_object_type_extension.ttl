  rdfs:isDefinedBy esmat:essampletype ;
  rdfs:label "Cell culture"@en ;
  skos:broader msot:analyticalpreparation ;
  skos:broader msot:bundlebiomeaggregation ;
  skos:definition "a collection of cells are grown under controlled conditions, generally outside of their natural environment"@en ;
